# vim:syntax=sh

enter ./python
  export LITREPL_AI_AUXDIR_OLD=$LITREPL_AI_AUXDIR
  export LITREPL_AI_AUXDIR=$PROJECT_ROOT/_litrepl/ai/python
  export LITREPL_PYTHON_AUXDIR_OLD=$LITREPL_PYTHON_AUXDIR
  export LITREPL_PYTHON_AUXDIR=$PROJECT_ROOT/_litrepl/python/python

leave ./python
  export LITREPL_AI_AUXDIR=$LITREPL_AI_AUXDIR_OLD
  unset LITREPL_AI_AUXDIR_OLD
  export LITREPL_PYTHON_AUXDIR=$LITREPL_PYTHON_AUXDIR_OLD
  unset LITREPL_PYTHON_AUXDIR_OLD

